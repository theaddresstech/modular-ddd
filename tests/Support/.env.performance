# Modular DDD Performance Configuration
MODULAR_DDD_ENABLED=true
MODULAR_DDD_EVENT_SOURCING_ENABLED=true
MODULAR_DDD_EVENT_SOURCING_SNAPSHOTS_ENABLED=true
MODULAR_DDD_EVENT_SOURCING_SNAPSHOTS_THRESHOLD=10
